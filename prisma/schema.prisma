generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Peca {
  id         String   @id @default(auto()) @map("_id") @db.ObjectId
  nome       String
  categoria  String
  quantidade Int
  status     String
  descricao  String?
  createdAt  DateTime @default(now())
  updatedAt  DateTime @updatedAt

  @@map("pecas")
}
